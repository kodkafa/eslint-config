{
  "name": "@kodkafa/eslint-config",
  "version": "1.1.3",
  "description": "Shareable ESLint and Prettier configuration used by KODKAFA",
  "license": "ISC",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./react": "./dist/react.js",
    "./next": "./dist/next.js",
    "./remix": "./dist/remix.js",
    "./prettier-config": "./dist/prettier-config.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "keywords": [
